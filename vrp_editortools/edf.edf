<def name="vRPEditorTools">
	<!-- Crane -->
	<element name="cranStart" friendlyname="Start cran position" icon="Crane.png">
		<data name="position" type="coord3d" default="0,0,0" required="true"/>
		<object editorOnly="true" model="3474"/>
	</element>
	<element name="cranEnd" friendlyname="End cran position" icon="Crane.png">
		<data name="position" type="coord3d" default="0,0,0" required="true"/>
		<object editorOnly="true" model="3474"/>
		<object editorOnly="true" model="2932" posX="7.100" posY="6.200" posZ="-5.400" rotZ="0.0"/>
		<object editorOnly="true" model="2934" posX="0.300" posY="7.300" posZ="-5.400" rotZ="-270.0"/>
		<object editorOnly="true" model="2935" posX="0.300" posY="10.400" posZ="-5.400" rotZ="-270.0"/>
		<object editorOnly="true" model="2932" posX="0.300" posY="9.200" posZ="-2.500" rotZ="-270.0"/>
		<object editorOnly="true" model="2932" posX="-6.400" posY="-5.100" posZ="-5.400" rotZ="0.0"/>
		<object editorOnly="true" model="2935" posX="7.100" posY="-1.000" posZ="-5.400" rotZ="-180.0"/>
		<object editorOnly="true" model="2935" posX="-6.400" posY="2.300" posZ="-5.400" rotZ="-180.0"/>
		<object editorOnly="true" model="2935" posX="-6.400" posY="-1.600" posZ="-2.500" rotZ="-180.0"/>
		<object editorOnly="true" model="2934" posX="7.100" posY="4.600" posZ="-2.500" rotZ="-180.0"/>
		<object editorOnly="true" model="2932" posX="0.100" posY="-8.100" posZ="-5.500" rotZ="-270.0"/>
		<object editorOnly="true" model="2934" posX="0.100" posY="-8.100" posZ="-2.600" rotZ="-270.0"/>
		<object editorOnly="true" model="2932" posX="7.100" posY="-2.600" posZ="-2.500" rotZ="0.0"/>
		<object editorOnly="true" model="2935" posX="7.000" posY="-8.100" posZ="-5.400" rotZ="-180.0"/>
		<object editorOnly="true" model="2932" posX="-6.400" posY="9.500" posZ="-5.400" rotZ="0.0"/>
		<object editorOnly="true" model="2934" posX="-6.400" posY="5.600" posZ="-2.500" rotZ="-180.0"/>
	</element>
	
	<!-- Bus job -->
	<element name="bus_stop" friendlyname="Bus stop" icon="Bus.png">
		<data name="position" type="coord3d" default="0,0,0" required="true"/>
		<object editorOnly="true" model="1257"/>
		<marker editorOnly="true" type="cylinder" size="4" color="#ff0000cc"/>
		<object editorOnly="true" model="1229" posX="-1.5" posY="3.4" posZ="0.2"/>
		<data name="markerdistance" type="number" default="4.5"/>
		<data name="lines" type="string" default=""/>
		<data name="name" type="string" default=""/>
	</element>
	
	<!-- Gangarea -->
	<element name="gangarea" friendlyname="Gangarea" icon="Group.png">
		<data name="position" type="coord3d" default="0,0,0" required="true"/>
		<object editorOnly="true" model="7921"/>
		<data name="areaX" type="number" default="0"/>
		<data name="areaY" type="number" default="0"/>
		<data name="maxZ" type="number" default="30"/>
		<data name="width" type="number" default="10"/>
		<data name="height" type="number" default="10"/>
	</element>
	
	<script src="client.lua" type="client" />
	<script src="server.lua" type="server" />
</def>
